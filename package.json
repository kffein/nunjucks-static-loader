{
  "name": "nunjucks-html-string-loader",
  "version": "1.0.0-alpha",
  "description": "Nunjucks loader that renders HTML strings",
  "main": "index.js",
  "scripts": {
    "test": "standard && ava",
    "test:watch": "ava --watch",
    "lint": "standard | snazzy",
    "container": "export NAME=nunjucks WORKDIR=/$NAME; if docker ps -a | grep -q $NAME; then npm run -s container:start; else npm run -s container:run; fi",
    "container:start": "docker start -ai nunjucks",
    "container:run": "docker run --name=$NAME -w /$NAME -p 8080:8080 -v $(pwd):/$NAME -v $(pwd)/package.json:/package.json -v /root/.npm -v /node_modules -e \"NODE_PATH=/node_modules\" -e \"CHOKIDAR_USEPOLLING=1\" -e \"PATH=/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin:/node_modules/.bin\" --dns 8.8.8.8 --dns 8.8.4.4 --entrypoint=/bin/bash -it node:6",
    "container:rm": "docker rm -f nunjucks"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/langri-sha/nunjucks-html-string-loader.git"
  },
  "keywords": [
    "webpack",
    "loader",
    "nunjucks",
    "html",
    "string"
  ],
  "engines": {
    "node": "6"
  },
  "author": "Filip DupanoviÄ‡ <filip.dupanovic@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/langri-sha/nunjucks-html-string-loader/issues"
  },
  "homepage": "https://github.com/langri-sha/nunjucks-html-string-loader#readme",
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "ava": "^0.15.2",
    "commitizen": "^2.8.2",
    "cz-conventional-changelog": "^1.1.6",
    "memory-fs": "^0.3.0",
    "nunjucks": "^2.4.2",
    "snazzy": "^4.0.0",
    "standard": "^7.1.2",
    "webpack": "^1.13.1"
  },
  "peerDependencies": {
    "nunjucks": "^2.0.0"
  },
  "dependencies": {
    "loader-utils": "^0.2.15"
  }
}
